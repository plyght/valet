<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.valet.service</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/valet</string>
        <string>--config</string>
        <string>/Users/you/valet.toml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/valet.out.log</string>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/valet.err.log</string>
</dict>
</plist>
